<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.nextep.designer.core.model.impl.RepositoryProperty" table="REP_PROPERTIES"
		lazy="false">
		<id name="id" column="PROP_ID">
			<generator class="hilo" />
		</id>
		<property name="name" column="PROP_NAME" />
		<property name="value" column="PROP_VALUE"/>

	</class>
</hibernate-mapping>
